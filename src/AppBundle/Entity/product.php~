<?php

namespace AppBundle\Entity;

/**
 * product
 */
class product
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var string
     */
    private $proName;

    /**
     * @var string
     */
    private $proIntroduce;

    /**
     * @var string
     */
    private $proTime;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set proName
     *
     * @param string $proName
     *
     * @return product
     */
    public function setProName($proName)
    {
        $this->proName = $proName;

        return $this;
    }

    /**
     * Get proName
     *
     * @return string
     */
    public function getProName()
    {
        return $this->proName;
    }

    /**
     * Set proIntroduce
     *
     * @param text $proIntroduce
     *
     * @return product
     */
    public function setProIntroduce($proIntroduce)
    {
        $this->proIntroduce = $proIntroduce;

        return $this;
    }

    /**
     * Get proIntroduce
     *
     * @return string
     */
    public function getProIntroduce()
    {
        return $this->proIntroduce;
    }

    /**
     * Set proTime
     *
     * @param string $proTime
     *
     * @return product
     */
    public function setProTime($proTime)
    {
        $this->proTime = $proTime;

        return $this;
    }

    /**
     * Get proTime
     *
     * @return string
     */
    public function getProTime()
    {
        return $this->proTime;
    }
}
